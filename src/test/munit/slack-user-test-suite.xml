		<munit:validation >
			<munit-tools:verify-call doc:name="Verify call" doc:id="eb8afe92-d537-4c6a-963e-fff960287c14" processor="ee:transform" atLeast="1">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="Transform Message" attributeName="doc:name" />
					<munit-tools:with-attribute whereValue="e51933af-1a6c-4978-a51a-d3c683d14b8e" attributeName="doc:id" />
				</munit-tools:with-attributes>
			</munit-tools:verify-call>
		</munit:validation>
	</munit:test>


</mule>
